<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4f4f4"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/ps_bg_box" >
       	<LinearLayout
       		android:layout_marginTop="14dp"
        	android:layout_marginLeft="14dp"
        	android:layout_gravity="center"
        	android:gravity="center"
        	android:layout_marginRight="14dp"
        	android:layout_marginBottom="14dp"
        	android:layout_width="match_parent"
        	android:layout_height="match_parent"
        	android:orientation="vertical"
        	android:background="@drawable/rv_img_line_complete" >

       		<LinearLayout
       		    android:layout_width="match_parent"
       		    android:layout_height="wrap_content"
       		    android:layout_marginLeft="25dp"
       		    android:layout_marginRight="25dp"
       		    android:orientation="vertical" >

       			<ImageView
       			    android:layout_gravity="center"
       			    android:id="@+id/title_img"
       			    android:layout_width="wrap_content"
       			    android:layout_height="wrap_content"
       			    android:src="@drawable/rv_img_complete" />

       			<TextView
       			    android:id="@+id/textView1"
       			    android:layout_width="wrap_content"
       			    android:layout_height="wrap_content"
       			    android:layout_gravity="center"
       			    android:layout_marginTop="11dp"
       			    android:text="@string/completed_reserv"
       			    android:textColor="#CF6BA9"
       			    android:textStyle="bold"
       			    android:textSize="18sp" />
       		</LinearLayout>
       		<LinearLayout
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_marginTop="30dp"
			        android:layout_marginLeft="25dp"
			        android:layout_marginRight="25dp"
			        android:orientation="horizontal">
			        <LinearLayout 
			            android:layout_width="match_parent"
			            android:layout_height="wrap_content"
			            android:layout_weight="1"
			            android:gravity="center"
			            android:layout_gravity="center"
			            android:background="@drawable/dash_bg"/>
			        <ImageView
				        android:id="@+id/title_img"
				        android:layout_marginLeft="18dp"
				        android:layout_marginRight="18dp"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:layout_gravity="center"
				        android:src="@drawable/rv_title_reservation" />
			        <LinearLayout 
			            android:layout_width="match_parent"
			            android:layout_height="wrap_content"
			            android:layout_weight="1"
			            android:gravity="center"
			            android:layout_gravity="center"
			            android:background="@drawable/dash_bg"/>
			    </LinearLayout>
			
       			
    			
       		
			   	<LinearLayout
			            android:layout_width="match_parent"
			            android:layout_height="wrap_content"
			            android:layout_marginLeft="25dp"
			            android:layout_marginRight="25dp"
			            android:layout_marginTop="15dp"
			            android:layout_marginBottom="15dp"
			            android:orientation="horizontal" >

			            <TextView
			                android:layout_width="match_parent"
			                android:layout_height="wrap_content"
			                android:layout_weight="1"
			                android:text="@string/sitter"
			                android:textColor="#4d4d4d"
			                android:textSize="15sp" />

			            <TextView
			                android:id="@+id/resv_comp_name"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:textColor="#4d4d4d"
			                android:textSize="15sp" />
			        </LinearLayout>
			        <LinearLayout
			            android:layout_marginLeft="25dp"
			            android:layout_marginRight="25dp"
			            android:layout_width="match_parent"
			            android:layout_height="1dp"
			            android:background="#e5e5e5"/>
			    <LinearLayout
			            android:layout_width="match_parent"
			            android:layout_height="wrap_content"
			            android:layout_marginLeft="25dp"
			            android:layout_marginRight="25dp"
			            android:layout_marginTop="15dp"
			            android:layout_marginBottom="15dp"
			            android:orientation="horizontal" >

			            <TextView
			                android:layout_width="match_parent"
			                android:layout_height="wrap_content"
			                android:layout_weight="1"
			                android:text="@string/reserv_phone"
			                android:textColor="#4d4d4d"
			                android:textSize="15sp" />

			            <TextView
			                android:id="@+id/resv_compted_phone"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:textColor="#4d4d4d"
			                android:textSize="15sp" />
			            <ImageButton
			               
	        				android:id="@+id/resv_compted_phone_btn"
	        				android:layout_width="20dp"
	    					android:layout_height="20dp"
	    					
	    					android:background="@drawable/resv_compted_phone_btn"/>
			            
			        </LinearLayout>
			        <LinearLayout
			            android:layout_marginLeft="25dp"
			            android:layout_marginRight="25dp"
			            android:layout_width="match_parent"
			            android:layout_height="1dp"
			            android:background="#e5e5e5"/>
			        <LinearLayout
			            android:layout_width="match_parent"
			            android:layout_height="wrap_content"
			            android:layout_marginLeft="25dp"
			            android:layout_marginRight="25dp"
			            android:layout_marginTop="15dp"
			            android:layout_marginBottom="15dp"
			            android:orientation="horizontal" >

			            <TextView
			                android:layout_width="match_parent"
			                android:layout_height="wrap_content"
			                android:layout_weight="1"
			                android:text="@string/address"
			                android:textColor="#4d4d4d"
			                android:textSize="15sp" />

			            <TextView
			                android:id="@+id/resv_compted_address"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:textColor="#4d4d4d"
			                android:textSize="15sp" />
			        </LinearLayout>
			 
			        
			        <LinearLayout
			            android:layout_marginLeft="25dp"
			            android:layout_marginRight="25dp"
			            android:layout_width="match_parent"
			            android:layout_height="1dp"
			            android:background="#e5e5e5"/>

			        <TextView
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_marginBottom="37dp"
			            android:layout_marginLeft="25dp"
			            android:layout_marginRight="25dp"
			            android:layout_marginTop="15dp"
			            android:text="@string/reserve_alm"
			            android:textColor="#4d4d4d"
			            android:textSize="13sp" />

			        <Button
			            android:id="@+id/resv_compted_btn"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:background="@drawable/su_btn_pink"
			            android:padding="10dp"
			            android:text="@string/return_reserve"
			            android:textColor="#ffffff" />

		</LinearLayout>
       	    
    </LinearLayout>

</LinearLayout>
