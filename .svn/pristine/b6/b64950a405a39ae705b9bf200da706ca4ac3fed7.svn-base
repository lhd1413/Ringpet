<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/join_background"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="#fffdf1"
		 	android:layout_marginLeft="10dp"
		 	android:layout_marginRight="10dp"/>

        <ImageView
            android:id="@+id/title_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="14dp"
            android:src="@drawable/su_img_member" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="14dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="17dp"
            android:background="@drawable/sign_dash"
            android:gravity="center">
			</LinearLayout>
		
		<ScrollView
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:fillViewport="true">
			
			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:focusable="true" 
	        	android:focusableInTouchMode="true"
	        	android:orientation="vertical"
	        	>
				<requestFocus></requestFocus>

				<LinearLayout
				    android:layout_width="match_parent"
				    android:layout_height="wrap_content"
				    android:layout_marginLeft="19dp"
				    android:layout_marginRight="19dp"
				    android:gravity="center"
				    android:background="@drawable/ps_bg_box"
				    android:layout_marginBottom="15dp"
				    android:orientation="vertical" >

				    <LinearLayout
				        android:layout_marginTop="18dp"
				        android:layout_width="56dp"
				        android:layout_height="56dp"
				        android:id="@+id/lv_profile"
				        android:layout_marginBottom="10dp"
				        android:background="@drawable/ellipse"
				        android:gravity="center" >

						<ImageView
						    android:id="@+id/img_profile"
						    android:layout_width="wrap_content"
						    android:layout_height="wrap_content"
						    android:src="@drawable/su_ic_profile" />
					</LinearLayout>

				    <TextView
				        android:id="@+id/textView1"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:text="@string/sign_alert"
				        android:textColor="#aaaaaa"
				        android:textSize="14sp" />
				    <LinearLayout
				        android:layout_width="match_parent"
				        android:layout_height="wrap_content"
				        android:layout_marginTop="15dp"
				        android:layout_marginLeft="31dp"
				        android:layout_marginRight="31dp"
				        android:orientation="horizontal">
				        <LinearLayout 
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_weight="1"
				            android:gravity="center"
				            android:layout_gravity="center"
				            android:background="@drawable/dash_bg"/>
	
				        <ImageView
				            android:layout_width="wrap_content"
				            android:layout_height="wrap_content"
				            android:layout_gravity="center"
				            android:layout_marginLeft="18dp"
				            android:layout_marginRight="18dp"
				            android:src="@drawable/su_img_profile" />
	
				        <LinearLayout 
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_weight="1"
				            android:gravity="center"
				            android:layout_gravity="center"
				            android:background="@drawable/dash_bg"/>
				    </LinearLayout>
				    <LinearLayout
				        android:layout_marginTop="4dp"
				        android:layout_marginLeft="31dp"
				        android:layout_marginRight="31dp"
				        android:layout_width="match_parent"
				        android:layout_height="wrap_content"
				        android:orientation="vertical" >
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/name"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/su_edit_name"
				                android:layout_width="wrap_content"
				        		android:background="#00000000"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:singleLine="true"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/id"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/su_edit_id"
				                android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
				                android:layout_width="wrap_content"
				                android:maxLength="12"
				        		android:background="#00000000"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:singleLine="true"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            <Button
				                android:id="@+id/su_chk_id_btn"
				                android:layout_width="wrap_content"
				                android:layout_height="wrap_content"
				                android:padding="10dp"
			            		android:textColor="#ffffff"
			            		android:background="@drawable/su_btn_pink"
				                android:text="@string/check_id" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/pw"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/su_edit_pw"
				                android:layout_width="wrap_content"
				        		android:background="#00000000"
				        		android:singleLine="true"
				        		android:password="true"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/pw_confirm"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/su_edit_pw_confirm"
				                android:layout_width="wrap_content"
				        		android:background="#00000000"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:singleLine="true"
				                android:password="true"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/address"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/su_edit_address"
				                android:layout_width="wrap_content"
				        		android:background="#00000000"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							<LinearLayout 
				                android:layout_width="wrap_content"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:orientation="vertical">
				                <LinearLayout
				                    android:layout_width="match_parent"
				                    android:layout_height="wrap_content">
				                    <TextView
						                android:layout_width="87dp"
						                android:layout_height="wrap_content"
						                android:text="@string/hp"
						                android:textColor="#aaaaaa"
						                android:textSize="14sp" />
				                    <EditText
				                		android:id="@+id/su_edit_phone"
				               			android:layout_width="wrap_content"
						        		android:background="#00000000"
						                android:layout_height="wrap_content"
						                android:layout_weight="1"
						                android:ems="10"
						                android:textColor="#4d4d4d"
						                android:textSize="15sp" />
						            <Button
						                android:id="@+id/su_chk_phone_btn"
						                android:layout_width="wrap_content"
						                android:layout_height="wrap_content"
						                android:padding="10dp"
					            		android:textColor="#ffffff"
					            		android:background="@drawable/su_btn_pink"
						                android:text="@string/check_phone" />
				                </LinearLayout>
				                <LinearLayout
				                    android:id="@+id/confirm_phone"
				                    android:layout_width="match_parent"
				                    android:layout_marginTop="5dp"
				                    android:visibility="invisible"
				                    android:layout_height="wrap_content">
				                    <LinearLayout
						                android:layout_width="87dp"
						                android:layout_height="wrap_content"/>
				                    <EditText
				                		android:id="@+id/su_edit_phone_confirm"
				               			android:layout_width="wrap_content"
						        		android:background="#00000000"
						                android:layout_height="wrap_content"
						                android:inputType="number"
						                android:layout_weight="1"
						                android:ems="10"
						                android:textColor="#4d4d4d"
						                android:textSize="15sp" />
						            <Button
						                android:id="@+id/su_chk_captcha_btn"
						                android:layout_width="wrap_content"
						                android:layout_height="wrap_content"
						                android:inputType="number"
						                android:padding="10dp"
					            		android:textColor="#ffffff"
					            		android:background="@drawable/su_btn_pink"
						                android:text="@string/check_confirm" />
				                </LinearLayout>
				                
				            </LinearLayout>
				            

				            
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:layout_marginBottom="15dp"
				            android:background="#e5e5e5"/>
				    </LinearLayout>

				</LinearLayout>

				<LinearLayout
				    android:id="@+id/lv_sign"
				    android:layout_gravity="right"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:layout_marginRight="19dp"
				    android:layout_marginBottom="10dp"
				    android:orientation="vertical" >
				    <ImageView
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        
				        android:src="@drawable/su_btn_next" />
				    <LinearLayout
				        android:layout_marginTop="6dp"
				        android:background="@drawable/big_dash"
				        android:layout_width="match_parent"
				   	 	android:layout_height="wrap_content">
				        
				    </LinearLayout>
				</LinearLayout>		
			</LinearLayout>
	    </ScrollView>
    </LinearLayout>

    

</LinearLayout>
