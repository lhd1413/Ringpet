<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mintfish.ringpet"
    android:versionCode="3"
    android:versionName="1.5" >
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        <activity
            android:name="com.mintfish.ringpet.signup.SignupPetSitterInformationActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.mintfish.ringpet.signup.SignupTypeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.mintfish.ringpet.signup.SignupPetSitterPetActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.mintfish.ringpet.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.mintfish.ringpet.FrameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mintfish.ringpet.SplashScreenActivity"/>
        <activity android:name="com.mintfish.ringpet.UserGuideGuestActivity"
            	  android:noHistory="true"/>   
        <activity android:name="com.mintfish.ringpet.UserGuidePetsitterActivity"
            	  android:noHistory="true"/>  
    </application>
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.CAMERA"/>
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.FLASHLIGHT"/>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
	<uses-permission android:name="android.permission.READ_CONTACTS" />
	<uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-feature android:name="android.hardware.camera" android:required="false"/> 
	<uses-feature android:name="android.hardware.camera.autofocus" android:required="false"/>
	<uses-feature android:name="android.hardware.telephony" android:required="false"/>
</manifest>
