<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/join_background"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="#fffdf1"
		 	android:layout_marginLeft="10dp"
		 	android:layout_marginRight="10dp"/>
        <ImageView
            android:id="@+id/title_img"
            android:layout_marginTop="14dp"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/su_img_member" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="14dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="17dp"
            android:background="@drawable/sign_dash"
            android:gravity="center">
			</LinearLayout>
		
		<ScrollView
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:fillViewport="true">
			
			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:focusable="true" 
	        	android:focusableInTouchMode="true"
	        	android:orientation="vertical"
	        	>
				<requestFocus></requestFocus>

				<LinearLayout
				    android:layout_width="match_parent"
				    android:layout_height="wrap_content"
				    android:layout_marginLeft="19dp"
				    android:layout_marginRight="19dp"
				    android:gravity="center"
				    android:background="@drawable/ps_bg_box"
				    android:layout_marginBottom="15dp"
				    android:orientation="vertical" >

				    <LinearLayout
				        android:id="@+id/psp_lv_dog_profile"
				        android:layout_marginTop="18dp"
				        android:layout_width="56dp"
				        android:layout_height="56dp"
				        android:layout_marginBottom="10dp"
				        android:background="@drawable/ellipse"
				        android:gravity="center" >

						<ImageView
						    android:id="@+id/psp_img_dog_profile"
						    android:layout_width="wrap_content"
						    android:layout_height="wrap_content"
						    android:src="@drawable/su_ic_pet" />
					</LinearLayout>

				    <TextView
				        android:id="@+id/textView1"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:text="@string/pet_alert"
				        android:textColor="#aaaaaa"
				        android:textSize="14sp" />
				    <LinearLayout
				        android:layout_width="match_parent"
				        android:layout_height="wrap_content"
				        android:layout_marginTop="15dp"
				        android:layout_marginLeft="31dp"
				        android:layout_marginRight="31dp"
				        android:orientation="horizontal">
				        <LinearLayout 
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_weight="1"
				            android:gravity="center"
				            android:layout_gravity="center"
				            android:background="@drawable/dash_bg"/>
	
				        <ImageView
				            android:id="@+id/title_img"
				            android:layout_width="wrap_content"
				            android:layout_height="wrap_content"
				            android:layout_gravity="center"
				            android:layout_marginLeft="18dp"
				            android:layout_marginRight="18dp"
				            android:src="@drawable/su_img_pet" />
	
				        <LinearLayout 
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_weight="1"
				            android:gravity="center"
				            android:layout_gravity="center"
				            android:background="@drawable/dash_bg"/>
				    </LinearLayout>
				    <LinearLayout
				        android:layout_marginTop="4dp"
				        android:layout_marginLeft="31dp"
				        android:layout_marginRight="31dp"
				        android:layout_width="match_parent"
				        android:layout_height="wrap_content"
				        android:orientation="vertical" >
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/name"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/psp_pt_name"
				                android:singleLine="true"
				                android:layout_width="wrap_content"
				        		android:background="#00000000"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/age_type"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/psp_pt_age"
				                android:layout_width="wrap_content"
				        		android:background="#00000000"
				        		android:singleLine="true"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/dog_type"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/psp_pt_type"
				                android:singleLine="true"
				                android:layout_width="wrap_content"
				        		android:background="#00000000"
				                android:layout_height="wrap_content"
				                android:layout_weight="1"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/sex"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />
				            <RadioGroup
				                android:id="@+id/psp_pt_sex"
					            android:layout_width="match_parent"
					            android:layout_height="wrap_content"
					            android:orientation="horizontal" >

					            <RadioButton
					                android:id="@+id/radioButton1"
					                android:layout_width="60dp"
					                android:layout_height="wrap_content"
					                android:layout_gravity="center"
					                android:button="@drawable/radio_purchase"
					                android:checked="true"
					                android:paddingLeft="7dp"
					                android:text="남"
					                android:textColor="#4d4d4d"
					                android:textSize="14sp" />
	
						        <RadioButton
						            android:id="@+id/radioButton2"
						            android:layout_width="60dp"
						            android:layout_height="wrap_content"
						            android:button="@drawable/radio_purchase"
						            android:paddingLeft="7dp"
						            android:text="여"
						            android:textColor="#4d4d4d"
						            android:textSize="14sp" />

			            	</RadioGroup>
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/size"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />
				            <RadioGroup
				                android:id="@+id/psp_pt_size"
					            android:layout_width="match_parent"
					            android:layout_height="wrap_content"
					            android:orientation="horizontal" >

					            <RadioButton
					                android:id="@+id/radioButton1"
					                android:layout_width="60dp"
					                android:checked="true"
					                android:layout_height="wrap_content"
					                android:layout_gravity="center"
					                android:button="@drawable/radio_purchase"
					                android:paddingLeft="7dp"
					                android:text="@string/size_middle"
					                android:textColor="#4d4d4d"
					                android:textSize="14sp" />
	
						        <RadioButton
						            android:id="@+id/radioButton2"
						            android:layout_width="60dp"
						            android:layout_height="wrap_content"
						            android:button="@drawable/radio_purchase"
						            android:paddingLeft="7dp"
						            android:text="@string/size_center"
						            android:textColor="#4d4d4d"
						            android:textSize="14sp" />
						        
						        <RadioButton
						            android:id="@+id/radioButton2"
						            android:layout_width="60dp"
						            android:layout_height="wrap_content"
						            android:button="@drawable/radio_purchase"
						            android:paddingLeft="7dp"
						            android:text="@string/size_big"
						            android:textColor="#4d4d4d"
						            android:textSize="14sp" />

			            	</RadioGroup>
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/inj_val"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />
				            <RadioGroup
				                android:id="@+id/psp_pt_step1"
					            android:layout_width="match_parent"
					            android:layout_height="wrap_content"
					            android:orientation="horizontal" >

					            <RadioButton
					                android:id="@+id/radioButton1"
					                android:layout_width="60dp"
					                android:checked="true"
					                android:layout_height="wrap_content"
					                android:layout_gravity="center"
					                android:button="@drawable/radio_purchase"
					                android:paddingLeft="7dp"
					                android:text="@string/step1"
					                android:textColor="#4d4d4d"
					                android:textSize="14sp" />
	
						        <RadioButton
						            android:id="@+id/radioButton2"
						            android:layout_width="60dp"
						            android:layout_height="wrap_content"
						            android:button="@drawable/radio_purchase"
						            android:paddingLeft="7dp"
						            android:text="@string/step2"
						            android:textColor="#4d4d4d"
						            android:textSize="14sp" />
						        
						        <RadioButton
						            android:id="@+id/radioButton2"
						            android:layout_width="60dp"
						            android:layout_height="wrap_content"
						            android:button="@drawable/radio_purchase"
						            android:paddingLeft="7dp"
						            android:text="@string/step3"
						            android:textColor="#4d4d4d"
						            android:textSize="14sp" />
						        
			            	</RadioGroup>

				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/crazy_type"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <RadioGroup
				                android:id="@+id/psp_pt_step2"
				                android:layout_width="match_parent"
				                android:layout_height="wrap_content"
				                android:orientation="horizontal" >

				                <RadioButton
				                    android:id="@+id/radioButton1"
				                    android:layout_width="60dp"
				                    android:checked="true"
				                    android:layout_height="wrap_content"
				                    android:layout_gravity="center"
				                    android:button="@drawable/radio_purchase"
				                    android:paddingLeft="7dp"
				                    android:text="@string/yes"
				                    android:textColor="#4d4d4d"
				                    android:textSize="14sp" />

				                <RadioButton
				                    android:id="@+id/radioButton2"
				                    android:layout_width="60dp"
				                    android:layout_height="wrap_content"
				                    android:layout_weight="0.44"
				                    android:button="@drawable/radio_purchase"
				                    android:paddingLeft="7dp"
				                    android:text="@string/no"
				                    android:textColor="#4d4d4d"
				                    android:textSize="14sp" />
				            </RadioGroup>
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/sex_type"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <RadioGroup
				                android:id="@+id/psp_pt_step3"
				                android:layout_width="match_parent"
				                android:layout_height="wrap_content"
				                android:orientation="horizontal" >

				                <RadioButton
				                    android:id="@+id/radioButton1"
				                    android:layout_width="60dp"
				                    android:checked="true"
				                    android:layout_height="wrap_content"
				                    android:layout_gravity="center"
				                    android:button="@drawable/radio_purchase"
				                    android:paddingLeft="7dp"
				                    android:text="@string/yes"
				                    android:textColor="#4d4d4d"
				                    android:textSize="14sp" />

				                <RadioButton
				                    android:id="@+id/radioButton2"
				                    android:layout_width="60dp"
				                    android:layout_height="wrap_content"
				                    android:layout_weight="0.44"
				                    android:button="@drawable/radio_purchase"
				                    android:paddingLeft="7dp"
				                    android:text="@string/no"
				                    android:textColor="#4d4d4d"
				                    android:textSize="14sp" />
				            </RadioGroup>
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="horizontal">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/heart_type"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <RadioGroup
				                android:id="@+id/psp_pt_step4"
				                android:layout_width="match_parent"
				                android:layout_height="wrap_content"
				                android:orientation="horizontal" >

				                <RadioButton
				                    android:id="@+id/radioButton1"
				                    android:layout_width="60dp"
				                    android:checked="true"
				                    android:layout_height="wrap_content"
				                    android:layout_gravity="center"
				                    android:button="@drawable/radio_purchase"
				                    android:paddingLeft="7dp"
				                    android:text="@string/yes"
				                    android:textColor="#4d4d4d"
				                    android:textSize="14sp" />

				                <RadioButton
				                    android:id="@+id/radioButton2"
				                    android:layout_width="60dp"
				                    android:layout_height="wrap_content"
				                    android:layout_weight="0.44"
				                    android:button="@drawable/radio_purchase"
				                    android:paddingLeft="7dp"
				                    android:text="@string/no"
				                    android:textColor="#4d4d4d"
				                    android:textSize="14sp" />
				            </RadioGroup>
				            
				        </LinearLayout>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="1dp"
				            android:background="#e5e5e5"/>
				        <LinearLayout
				            android:layout_width="match_parent"
				            android:layout_height="wrap_content"
				            android:layout_marginTop="15dp"
				            android:layout_marginBottom="15dp"
				            android:orientation="vertical">
							
				            <TextView
				                android:layout_width="87dp"
				                android:layout_height="wrap_content"
				                android:text="@string/introduction_my_pet"
				                android:textColor="#aaaaaa"
				                android:textSize="14sp" />

				            <EditText
				                android:id="@+id/psp_pt_introduce"
				                android:layout_marginTop="12dp"
				                android:layout_width="match_parent"
				        		android:background="#00000000"
				                android:layout_height="wrap_content"
				                android:ems="10"
				                android:textColor="#4d4d4d"
				                android:textSize="15sp" />
				            
				        </LinearLayout>
				        
				      

				    </LinearLayout>

				</LinearLayout>
				  <LinearLayout
				    	android:id="@+id/lv_sign"
				    	android:layout_gravity="right"
				    	android:layout_width="wrap_content"
				    	android:layout_height="wrap_content"
				    	android:layout_marginRight="19dp"
				    	android:layout_marginBottom="10dp"
				    	android:orientation="vertical" >
				    		<ImageView
				        	android:layout_width="wrap_content"
				        	android:layout_height="wrap_content"   
				        	android:src="@drawable/su_btn_next" />
				    <LinearLayout
				        android:layout_marginTop="6dp"
				        android:background="@drawable/big_dash"
				        android:layout_width="match_parent"
				   	 	android:layout_height="wrap_content">
				        
				    </LinearLayout>
					</LinearLayout>	
			</LinearLayout>
	    </ScrollView>
    </LinearLayout>

    

</LinearLayout>